{
  "name": "carefordesign",
  "version": "1.0.0",
  "description": "Kaatje Mullebrouck portfolio",
  "main": "index.js",
  "scripts": {
    "all": "npm run build && npm start",
    "build": "npm run lint; npm run js && npm run css",
    "postinstall": "npm run build",
    "js": "NODE_ENV=production browserify -d -t reactify -g envify -g uglifyify -o public/js/bundle.js jsx/app.js",
    "css": "node-sass scss/app.scss public/css/app.css",
    "lint": "eslint --quiet jsx/",
    "start": "node --debug app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Kaatje and Jorge",
  "license": "ISC",
  "engines": {
    "node": "0.12.4",
    "npm": "2.10.1"
  },
  "dependencies": {
    "body-parser": "^1.13.0",
    "browserify": "^10.2.4",
    "ejs": "^2.3.1",
    "envify": "^3.4.0",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "esprima-fb": "^15001.1.0-dev-harmony-fb",
    "express": "^4.12.4",
    "node-sass": "^3.2.0",
    "react": "^0.13.3",
    "react-tools": "^0.13.3",
    "reactify": "^1.1.1",
    "superagent": "^1.2.0",
    "uglifyify": "^3.0.1"
  }
}
